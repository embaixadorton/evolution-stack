# Chave de autenticação usada para validação de chamadas na API
AUTHENTICATION_API_KEY=

# Exibe instâncias mesmo com autenticação ativada
AUTHENTICATION_EXPOSE_IN_FETCH_INSTANCES=true

# Habilita ou desabilita cache local no servidor
CACHE_LOCAL_ENABLED=false

# Habilita cache com Redis
CACHE_REDIS_ENABLED=true

# Prefixo para as chaves armazenadas no Redis
CACHE_REDIS_PREFIX_KEY=evolution_v2

# Salva instâncias no Redis (false = não salva)
CACHE_REDIS_SAVE_INSTANCES=false

# URI de conexão com o Redis utilizado como cache
CACHE_REDIS_URI=redis://evo_redis:6379/1

# Habilita integração com Chatwoot (plataforma de atendimento)
CHATWOOT_ENABLED=false

# Conexão com o banco de dados do Chatwoot
CHATWOOT_IMPORT_DATABASE_CONNECTION_URI=postgresql://user:password@host:5432/chatwoot?sslmode=disable

# Substitui mensagens de mídia por um texto padrão no Chatwoot
CHATWOOT_IMPORT_PLACEHOLDER_MEDIA_MESSAGE=true

# Permite exclusão de mensagens no Chatwoot
CHATWOOT_MESSAGE_DELETE=true

# Marca mensagens como lidas no Chatwoot
CHATWOOT_MESSAGE_READ=true

# Nome do cliente usado na conexão com o banco de dados
DATABASE_CONNECTION_CLIENT_NAME=evolution_v2

# URI de conexão com o banco de dados principal
DATABASE_CONNECTION_URI=postgresql://user:pass@host:5432/evolution

# Habilita o uso de banco de dados
DATABASE_ENABLED=true

# Tipo de banco de dados utilizado
DATABASE_PROVIDER=postgresql

# Salva dados de conversas no banco
DATABASE_SAVE_DATA_CHATS=true

# Salva dados de contatos no banco
DATABASE_SAVE_DATA_CONTACTS=true

# Salva histórico de interações no banco
DATABASE_SAVE_DATA_HISTORIC=true

# Salva dados da instância no banco
DATABASE_SAVE_DATA_INSTANCE=true

# Salva etiquetas no banco
DATABASE_SAVE_DATA_LABELS=true

# Salva novas mensagens no banco
DATABASE_SAVE_DATA_NEW_MESSAGE=true

# Salva atualizações em mensagens no banco
DATABASE_SAVE_MESSAGE_UPDATE=true

# Remove a instância (utilizado em testes ou ambientes específicos)
DEL_INSTANCE=false

# Habilita integração com a plataforma Dify (IA)
DIFY_ENABLED=true

# Define o idioma padrão da aplicação
LANGUAGE=pt-br

# Nível de log específico do Baileys (conexão com WhatsApp)
LOG_BAILEYS=error

# Nível de log geral da aplicação
LOG_LEVEL=ERROR

# Habilita integração com a API da OpenAI
OPENAI_ENABLED=true

# Cor usada na geração de QR Codes para conexão de instâncias
QRCODE_COLOR=#175197

# Limite de QR Codes simultâneos disponíveis
QRCODE_LIMIT=30

# Habilita uso do RabbitMQ para filas de mensagens
RABBITMQ_ENABLED=true

# Define se o RabbitMQ será compartilhado entre múltiplas instâncias
RABBITMQ_GLOBAL_ENABLED=true

# URI de conexão com o RabbitMQ
RABBITMQ_URI=amqp://user:pass@rabbitmq:5672/rabbitmq

# Chave de acesso para o armazenamento em S3
S3_ACCESS_KEY=

# Nome do bucket utilizado no S3
S3_BUCKET=evolution

# Habilita o uso de armazenamento S3 (MinIO ou outro)
S3_ENABLED=true

# Endpoint de acesso ao serviço S3
S3_ENDPOINT=

# Porta usada pelo serviço S3
S3_PORT=443

# Chave secreta de acesso ao S3
S3_SECRET_KEY=

# Define se a conexão com o S3 usa SSL
S3_USE_SSL=true

# URL pública principal da aplicação
SERVER_URL=https://example.com

# Habilita uso do SQS (desabilitado por padrão)
SQS_ENABLED=false

# Define se a aplicação envia dados de telemetria
TELEMETRY=false

# Versão da API utilizada pelo Typebot
TYPEBOT_API_VERSION=latest

# Habilita integração com Typebot (plataforma de chatbot)
TYPEBOT_ENABLE=true

# Habilita suporte a WebSocket
WEBSOCKET_ENABLED=true

# Domínio público da aplicação Evolution (utilizado pelo Traefik)
EVOLUTION_DOMAIN=evo.example.com

# Domínio público usado para acessar os arquivos no MinIO
MINIO_DOMAIN=s3.example.com

# Domínio público para o painel do MinIO
MINIO_CONSOLE_DOMAIN=storage.example.com

# Nome do banco de dados PostgreSQL
POSTGRES_DB=evolution

# Senha do usuário do PostgreSQL
POSTGRES_PASSWORD=

# Nome de usuário root do MinIO
MINIO_ROOT_USER=

# Senha do usuário root do MinIO
MINIO_ROOT_PASSWORD=

# URL para redirecionamento do painel do MinIO
MINIO_BROWSER_REDIRECT_URL=https://storage.example.com

# URL de acesso externo ao MinIO
MINIO_SERVER_URL=https://s3.example.com

# Cookie usado pelo cluster RabbitMQ (deve ser igual entre nós em produção)
RABBITMQ_ERLANG_COOKIE=

# Nome do Virtual Host padrão no RabbitMQ
RABBITMQ_DEFAULT_VHOST=rabbitmq

# Nome de usuário padrão do RabbitMQ
RABBITMQ_DEFAULT_USER=

# Senha do usuário padrão do RabbitMQ
RABBITMQ_DEFAULT_PASS=
